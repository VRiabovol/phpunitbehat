{
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "VRiabovol/phpunitbehat",
        "version": "3.0",
        "type": "package",
        "source": {
          "url": "https://github.com/VRiabovol/phpunitbehat",
          "type": "git",
          "reference": "master"
        }
      }
    }
  ],
  "require": {
      "php": ">=7.0.0",
      "VRiabovol/phpunitbehat": "*",
      "phpunit/phpunit": "9.5",
      "symfony/dependency-injection": "^4.4"
  },
  "autoload": {
      "psr-4": {
          "PHPUnitBehat\\": "src"
      }
  },
  "autoload-dev": {
    "psr-4": {
        "PHPUnitBehat\\Tests\\": "tests"
    }
  }
}
